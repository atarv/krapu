fn t() -> I64 {
    1
}

fn print_i64(i: I64) {
    println(i64_to_str(i));
}

fn main() {
    // Functions can be used before their definition if they occur in the same scope.
    // I think this is called hoisting, at least in JavaScript terms.
    let f: I64 = t();

    fn t() -> I64 {
        2
    }

    print_i64(f); // Should print 2
}
