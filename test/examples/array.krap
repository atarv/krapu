fn main() {
    let arr: Str[] = ["incorrect", "not correct either", "Correct!"];
    println(arr[2]); // Should print correct!
    // Assignment can be chained
    arr[1] = arr[0] = "Now this is correct too!";
    println(arr[0]);
    println(arr[1]);

    // Arrays can even be multi-dimensional!
    let twoDim: I64[][] = [[1,2,3], [4,5,6], [7,8,9]];
    println(i64_to_str( twoDim[1][2] ));
}